entos/Documentos BigPink/bambinos/bambinos/all/src/components/Banner/Banner.js",[],"/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/components/CartaAlimentos/CartaAlimentos.js",[],"/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/components/CartaLitros/CartaLitros.js",["207"],"import React from 'react';\nimport { carta_litros } from '../../data/carta_litros';\nimport { Typography } from '../../global/fonts/typography';\nimport { ListItem } from '../ListItem/ListItem';\nimport LitrosImg from '../../global/img/litros/litros.png';\n\nexport const CartaLitros = () => {\n  return (\n    <>\n\n      <div className=\"m-4\">\n        <div className=\"mt-4 d-flex flex-row align-items-center  justify-content-between\">\n          <h2 className=\"title-card\" style={Typography.titleMenu}>\n            Los Litros Bambinos\n          </h2>\n          <img className=\"img-litros img-fluid\" src={LitrosImg} />\n        </div>\n        {\n          carta_litros.map((cat) => (\n            <ListItem key={cat.nombre} cat={cat} />\n          ))\n        }\n      </div>\n    </>\n  );\n};\n","/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/components/Footer/Footer.js",[],"/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/components/CartaBebidas/CartaBebidas.js",[],"/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/components/Banner/Header.js",["208"],"import React from 'react';\nimport BannerLogo from '../../global/img/banner.png';\n\nexport const Header = () => (\n  <div className=\"d-flex justify-content-center pt-4 pb-3\">\n    <div className=\"text-center\">\n      <img className=\"img-fluid-header w-10 h-5\" src={BannerLogo} />\n    </div>\n  </div>\n);\n","/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/components/Banner/Carrusel.js",[],"/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/global/fonts/typography.js",[],"/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/data/categorias_bebidas.js",[],"/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/data/categorias_alimentos.js",[],"/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/data/carta_litros.js",[],"/home/andrea/Documentos/Documentos BigPink/bambinos/bambinos/all/src/components/ListItem/ListItem.js",["209"],"import React, { useState } from 'react';\nimport { Typography } from '../../global/fonts/typography';\nimport { filterByCategory } from '../../helpers/filters';\nimport { CardBackgroundColor } from '../Cards/CardBackgroundColor';\nimport { CardImage } from '../Cards/CardImage';\nimport { Item } from './Item';\n\nexport const ListItem = ({ cat }) => {\n  const [select, setSelect] = useState(false);\n  const [nombre, setNombre] = useState('');\n\n  const setNameCat = (item, status) => {\n    setNombre(item);\n    if (item === nombre) {\n      setNombre('');\n      setSelect(!status);\n    }\n  };\n\n  return (\n    <>\n      <div className=\"card mt-3 mb-1\" >\n        {\n          cat.img ? (\n            <CardImage\n              cat={cat}\n              setNameCat={setNameCat}\n              select={select}\n              nombre={nombre}\n            />\n          ) : (\n            <CardBackgroundColor\n              cat={cat}\n              setNameCat={setNameCat}\n              select={select}\n              nombre={nombre}\n            />\n          )\n        }\n\n      </div>\n\n      {\n        (\n          <div\n            className={\n              nombre === cat.nombre ? 'animate__animated animate__fadeIn' : 'collapse'}>\n            <div className=\"mt-3\">\n              {\n                filterByCategory(cat.nombre, cat.tipo).map((rs) => (\n                  <Item key={rs.nombre} rs={rs} />\n                ))\n              }\n              <h3 style={Typography.plateTitleText}></h3>\n            </div>\n          </div>\n        )\n      }\n\n    </>\n  );\n};","/home/a